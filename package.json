{
  "name": "@nyaruka/flow-editor",
  "license": "AGPL-3.0",
  "repository": "git://github.com/nyaruka/floweditor.git",
  "version": "1.1.8",
  "description": "'Standalone flow editing tool designed for use within the RapidPro suite of messaging tools but can be adopted for use outside of that ecosystem.'",
  "browser": "umd/flow-editor.min.js",
  "unpkg": "umd/flow-editor.min.js",
  "author": "Nyaruka <code@nyaruka.com>",
  "files": [
    "umd"
  ],
  "scripts": {
    "test": "jest --maxWorkers=4 --coverage",
    "test:local": "jest",
    "test:watch": "npm run test:local -- --watch",
    "test:watch:coverage": "npm run test:local -- --watch --coverage",
    "test:coverage": "npm run test:local -- --coverage",
    "test:update": "npm run test:local -- --updateSnapshot",
    "start": "webpack-dev-server --config webpack/webpack.config.js --env.NODE_ENV=development --progress --colors",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "lint:es": "eslint '*/**/*.js'",
    "lint:ts": "tslint -c tslint.json 'src/**/*.{ts,tsx}' -e 'src/**/*.d.ts'",
    "clean:umd": "shx rm -rf umd",
    "clean:prev": "shx rm -rf preview/dist",
    "build:prev": "npm run clean:prev && webpack --config webpack/webpack.config.js --env.NODE_ENV=preview && npm run lambda:build",
    "build:umd": "npm run clean:umd & webpack --config webpack/webpack.config.js --env.NODE_ENV=production",
    "bundleStats:dev": "webpack --config webpack/webpack.config.js --env.NODE_ENV=development --profile --json > stats.json",
    "bundleStats:dev:windows": "webpack --config webpack/webpack.config.js --env.NODE_ENV=development --profile --json | Out-file 'stats.json' -Encoding OEM",
    "bundleStats:prod": "webpack --config webpack/webpack.config.js --env.NODE_ENV=production --profile --json > stats.json",
    "bundleStats:prod:windows": "webpack --config webpack/webpack.config.js --env.NODE_ENV=production --profile --json | Out-file 'stats.json' -Encoding OEM",
    "analyze": "webpack-bundle-analyzer stats.json ./dist -m 'server'",
    "runAnalyzer:dev": "npm run bundleStats:dev && npm run analyze",
    "runAnalyzer:dev:windows": "npm run bundleStats:dev:windows && npm run analyze",
    "runAnalyzer:prod": "npm run bundleStats:prod && npm run analyze",
    "runAnalyzer:prod:windows": "npm run bundleStats:prod:windows && npm run analyze",
    "preprettify": "prettier --debug-check '{,!(node_modules)/**/}*.@(ts|tsx)'",
    "prettify": "prettier --write 'src/**/*.{ts,tsx,json,js,css,md}'",
    "lambda:serve": "netlify-lambda serve lambda-src -p 8888",
    "lambda:build": "netlify-lambda build lambda-src",
    "tag": "git push --tags origin master",
    "preversion": "npm run test:local",
    "version": "npm run build:umd",
    "postversion": "npm run tag && npm run release",
    "precommit": "lint-staged",
    "release": "npm publish --access public"
  },
  "lint-staged": {
    "*.{ts,tsx,js,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "peerDependencies": {
    "react": ">=16.0",
    "react-dom": ">=16.0"
  },
  "devDependencies": {
    "@types/classnames": "2.2.4",
    "@types/common-tags": "1.4.0",
    "@types/core-js": "2.5.0",
    "@types/enzyme": "3.1.11",
    "@types/enzyme-adapter-react-16": "1.0.2",
