    "@types/uuid": "2.0.30",
    "@types/webpack-env": "1.13.1",
    "ajv": "6.1.1",
    "auto-bind": "1.2.0",
    "autoprefixer": "7.2.3",
    "awesome-typescript-loader": "3.4.1",
    "axios": "0.17.1",
    "babel-core": "6.26.0",
    "babel-minify-webpack-plugin": "0.2.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "classnames": "2.2.5",
    "compression-webpack-plugin": "0.4.0",
    "core-js": "2.5.1",
    "coveralls": "3.0.0",
