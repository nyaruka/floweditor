{
  "name": "flow-editor",
  "license": "AGPL-3.0",
  "repository": "git://github.com/nyaruka/floweditor.git",
  "version": "1.1.0",
  "description": "'Standalone flow editing tool designed for use within the RapidPro suite of messaging tools but can be adopted for use outside of that ecosystem.'",
  "main": "app.tsx",
  "author": "Nyaruka <code@nyaruka.com>",
  "scripts": {
    "test": "jest --maxWorkers=4 --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "test:local": "jest",
    "test:watch": "npm run test:local -- --watch",
    "test:coverage": "npm run test:local -- --coverage",
    "test:update": "npm run test:local -- --updateSnapshot",
    "start:dev": "webpack-dev-server --config webpack/webpack.config.js --env.NODE_ENV=dev --progress --colors",
    "start:prev": "webpack-dev-server --config webpack/webpack.config.js --port=9000 --env.NODE_ENV=prev --progress --colors",
    "lint:es": "eslint '*/**/*.js'",
    "lint:ts": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "build:dev": "webpack --config webpack/webpack.config.js --env.NODE_ENV=dev",
    "build:prev": "webpack --config webpack/webpack.config.js --env.NODE_ENV=prev",
    "build:prod": "webpack --config webpack/webpack.config.js --env.NODE_ENV=prod",
    "bundleStats:dev": "webpack --config webpack/webpack.config.js --env.NODE_ENV=dev --profile --json > stats.json",
    "bundleStats:dev:windows": "webpack --config webpack/webpack.config.js --env.NODE_ENV=dev --profile --json | Out-file 'stats.json' -Encoding OEM",
    "bundleStats:prod": "webpack --config webpack/webpack.config.js --env.NODE_ENV=prod --profile --json > stats.json",
    "bundleStats:prod:windows": "webpack --config webpack/webpack.config.js --env.NODE_ENV=prod --profile --json | Out-file 'stats.json' -Encoding OEM",
    "analyze": "webpack-bundle-analyzer stats.json ./dist -m 'server'",
    "runAnalyzer:dev": "npm run bundleStats:dev && npm run analyze",
    "runAnalyzer:dev:windows": "npm run bundleStats:dev:windows && npm run analyze",
    "runAnalyzer:prod": "npm run bundleStats:prod && npm run analyze",
    "runAnalyzer:prod:windows": "npm run bundleStats:prod:windows && npm run analyze",
    "preprettify": "prettier --debug-check '{,!(node_modules)/**/}*.@(ts|tsx)'",
    "prettify": "prettier --write 'src/**/*.{ts,tsx,json,js,css,md}'",
    "preversion": "npm run test:local",
    "version": "npm run build:prod",
    "postversion": "git push && git push --tags && npm publish",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{ts,tsx,js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "peerDependencies": {
    "react": "^16.0",
    "react-dom": "^16.0"
  },
  "devDependencies": {
    "@types/classnames": "2.2.3",
    "@types/core-js": "0.9.43",
    "@types/enzyme": "2.8.0",
    "@types/enzyme-adapter-react-16": "1.1.1",
    "@types/immutability-helper": "2.0.15",
    "@types/jest": "21.1.4",
    "@types/jquery": "2.0.40",
    "@types/node": "7.0.5",
    "@types/prop-types": "15.5.2",
    "@types/react": "16.0.27",
    "@types/react-addons-shallow-compare": "0.14.19",
    "@types/react-beautiful-dnd": "2.3.2",
    "@types/react-dom": "16.0.3",
